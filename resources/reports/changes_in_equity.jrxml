	<queryString>
<![CDATA[SELECT 
            fc.name as category,
            v.vote_code as code, 
            v.description as name, 
            (SUM(gj.credit) - SUM(gj.debit)) as balance
        FROM general_journals gj
        JOIN votes v ON gj.vote_id = v.id
        LEFT JOIN financial_categories fc ON v.financial_category_id = fc.id
        LEFT JOIN financial_category_classes fcc ON v.financial_category_class_id = fcc.id
        WHERE gj.trans_date <= $P{date_to}
        AND fcc.name = 'Equity'
        GROUP BY fc.name, v.id, v.description, v.vote_code
        HAVING (SUM(gj.credit) - SUM(gj.debit)) != 0
        ORDER BY fc.name, v.vote_code ASC]]>
	</queryString>
	<field name="category" class="java.lang.String"/>
	<field name="code" class="java.lang.String"/>
	<field name="name" class="java.lang.String"/>
	<field name="balance" class="java.math.BigDecimal"/>
    <variable name="equity_total" class="java.math.BigDecimal" calculation="Sum">
        <variableExpression><![CDATA[$F{balance}]]></variableExpression>
    </variable>
