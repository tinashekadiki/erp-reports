	<queryString>
		<![CDATA[SELECT 
            fcc.name as account_type,
            v.vote_code as account_code, 
            v.description as account_name, 
            CASE WHEN (SUM(gj.debit) - SUM(gj.credit)) > 0 THEN (SUM(gj.debit) - SUM(gj.credit)) ELSE 0 END as debit,
            CASE WHEN (SUM(gj.debit) - SUM(gj.credit)) < 0 THEN ABS(SUM(gj.debit) - SUM(gj.credit)) ELSE 0 END as credit
        FROM general_journals gj
        JOIN votes v ON gj.vote_id = v.id
        JOIN financial_category_classes fcc ON v.financial_category_class_id = fcc.id
        WHERE gj.trans_date <= $P{date_to}
        GROUP BY fcc.name, v.vote_code, v.description, fcc.reporting_order
        HAVING (SUM(gj.debit) - SUM(gj.credit)) != 0
        ORDER BY account_code ASC]]>
	</queryString>
