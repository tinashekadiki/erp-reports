{
    "name": "nexterp/jasper-reports",
    "description": "A Jasper Reports bridge for Laravel with Java 8 compatibility and pre-compiled templates.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tinashe Kadiki"
        }
    ],
    "require": {
        "php": ">=8.1",
        "geekcom/phpjasper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nexterp\\JasperReports\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nexterp\\JasperReports\\JasperReportsServiceProvider"
            ]
        }
    }
}